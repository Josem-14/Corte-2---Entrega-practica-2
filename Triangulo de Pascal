#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<vector<int>> triangulo;
    int num;

    cout << "Ingrese un nÃºmero: ";
    cin >> num;

    for (int i = 0; i < num; i++) {
        
        triangulo.push_back(vector<int>(i + 1, 1));

        
        for (int j = 1; j < i; j++) {
            triangulo[i][j] = triangulo[i - 1][j - 1] + triangulo[i - 1][j];
        }
    }

    for (int i = 0; i < num; i++) {
        for (int j = 0; j < triangulo[i].size(); j++) {
            cout <<triangulo[i][j] << "";
        }
        cout << endl;
    }

    return 0;
}
